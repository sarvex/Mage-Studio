{
  "name": "MageStudio",
  "version": "0.0.3",
  "description": "Mage Studio",
  "main": "main.js",
  "scripts": {
    "dev": "IS_DESKTOP=true node server/index",
    "dev:inspect": "IS_DESKTOP=true node --inspect server/index",
    "build": "next build",
    "start": "NODE_ENV=production node server/index",
